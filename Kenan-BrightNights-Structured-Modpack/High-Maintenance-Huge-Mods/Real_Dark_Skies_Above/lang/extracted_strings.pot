msgstr ""

#: .\NPC\TALK_MAQUIS_BIONIC_DOCTOR.json .\NPC\TALK_MAQUIS_ENGINEER.json
#: .\NPC\TALK_MAQUIS_QUARTERMASTER.json .\NPC\TALK_MAQUIS_SCIENTIST.json
#: .\NPC\TALK_RANDY_ERIKSON.json
msgid "Thanks."
msgstr ""

#: .\NPC\TALK_MAQUIS_BIONIC_DOCTOR.json .\NPC\TALK_MAQUIS_ENGINEER.json
#: .\NPC\TALK_MAQUIS_MECHANIC.json .\NPC\TALK_MAQUIS_MOTH_MUTANT.json
#: .\NPC\TALK_MAQUIS_SCIENTIST.json
