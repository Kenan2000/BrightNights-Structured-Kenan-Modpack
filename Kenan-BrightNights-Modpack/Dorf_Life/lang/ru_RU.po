msgid "underground breach"
msgstr "подземный пролом"

#: overmap_terrain.json
msgid "underground cavern"
msgstr "подземная пещера"

#: overmap_terrain.json
msgid "abandoned market"
msgstr "заброшенный рынок"

#: overmap_terrain.json
